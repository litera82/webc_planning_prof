    <record>
		<property name="evenement_id" fieldname="evenement_id" datatype="autoincrement"/>
        <property name="evenement_iTypeEvenementId" fieldname="evenement_iTypeEvenementId" datatype="int" required="true" />
        <property name="evenement_iUtilisateurId" fieldname="evenement_iUtilisateurId" datatype="int" required="true" />
        <property name="evenement_zLibelle" fieldname="evenement_zLibelle" datatype="string" required="true" maxlength="80"/>
        <property name="evenement_zDescription" fieldname="evenement_zDescription" datatype="string" />
        <property name="evenement_iStagiaire" fieldname="evenement_iStagiaire" datatype="int" />
