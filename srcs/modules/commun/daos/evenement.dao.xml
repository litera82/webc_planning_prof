        <property name="evenement_zDateHeureSaisie" fieldname="evenement_zDateHeureSaisie" datatype="string" />
        <property name="evenement_iDuree" fieldname="evenement_iDuree" datatype="int" />
        <property name="evenement_iDureeTypeId" fieldname="evenement_iDureeTypeId" datatype="int" />
        <property name="evenement_iPriorite" fieldname="evenement_iPriorite" datatype="int" required="true" />
        <property name="evenement_iRappel" fieldname="evenement_iRappel" datatype="int"/>
        <property name="evenement_iTypeRappelId" fieldname="evenement_iTypeRappelId" datatype="int"/>
        <property name="evenement_iStatut" fieldname="evenement_iStatut" datatype="int" required="true" />
        <property name="evenement_origine" fieldname="evenement_origine" datatype="int" default="0" />
        <property name="evenement_firstcours" fieldname="evenement_firstcours" datatype="int" default="0" />
        <property name="evenement_solde" fieldname="evenement_solde" datatype="string" required="false" />
        <property name="evenement_prevu" fieldname="evenement_prevu" datatype="string" required="false" />
        <property name="evenement_produit" fieldname="evenement_produit" datatype="string" required="false" />
